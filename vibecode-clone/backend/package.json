{
  "name": "@vibecode/backend",
  "version": "1.0.0",
  "description": "Backend API server for Vibecode Clone platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "setup:stripe": "stripe listen --forward-to localhost:8000/api/webhooks/stripe"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/cookie": "^9.2.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^5.7.0",
    "prisma": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.7.4",
    "dockerode": "^4.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "stripe": "^14.9.0",
    "aws-sdk": "^2.1504.0",
    "@supabase/supabase-js": "^2.38.5",
    "redis": "^4.6.11",
    "bull": "^4.12.2",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@google-ai/generativelanguage": "^2.5.0",
    "simple-git": "^3.20.0",
    "tar": "^6.2.0",
    "archiver": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.0.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nodemailer": "^6.4.14",
    "@types/multer": "^1.4.11",
    "@types/passport": "^1.0.16",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/speakeasy": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/tar": "^6.1.10",
    "@types/archiver": "^6.0.2",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.2",
