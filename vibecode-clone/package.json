{
  "name": "vibecode-clone",
  "version": "1.0.0",
  "description": "Complete platform replicating and improving Vibecode Terminal",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "ai-agent-manager",
    "shared"
  ],
  "scripts": {
    "demo": "node run-demo.js",
    "start": "node start.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:agents\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:agents": "cd ai-agent-manager && npm run dev",
    "build": "npm run build:shared && npm run build:frontend && npm run build:backend && npm run build:agents",
    "build:shared": "cd shared && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build:agents": "cd ai-agent-manager && npm run build",
    "test": "npm run test:frontend && npm run test:backend && npm run test:agents",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "test:agents": "cd ai-agent-manager && npm run test",
    "test:e2e": "playwright test",
    "db:setup": "cd backend && npm run db:setup",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed",
    "db:reset": "cd backend && npm run db:reset",
    "db:studio": "cd backend && npm run db:studio",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "setup:stripe": "cd backend && npm run setup:stripe",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "express": "^4.18.2",
    "better-sqlite3": "^9.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/vibecode-clone.git"
  },
  "author": "Your Organization",
  "license": "MIT"
}